<h1>ユーザー一覧じゃい</h1>
<br>
<table>
  <th>メルアド</th>
  <% @users.each do | user |%>
    <% unless user.admin? || user == current_user%>
      <tr>
        <td><%= user.email%></td>
        <td>
          <%= render 'follow_form', user: user %>
        </td>
      </tr>
      <% end %>
  <% end %>
</table>
